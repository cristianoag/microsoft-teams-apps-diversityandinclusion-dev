{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0.0",
  "id": "fe0dfdaa-f1f7-4d85-9fd1-35b4edf4ffd1",
  "packageName": "com.microsoft.teams.diconnect",
  "developer": {
    "name": "<<companyName>>",
    "websiteUrl": "<<websiteUrl>>",
    "privacyUrl": "<<privacyUrl>>",
    "termsOfUseUrl": "<<termsOfUseUrl>>"
  },
  "localizationInfo": {
    "defaultLanguageTag": "en"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Diversity and Inclusion",
    "full": "Diversity and Inclusion"
  },
  "description": {
    "short": "Discover or create employee groups focused on diversity and inclusion",
    "full": "An easy-to-browse interface helps employees find and join groups focused on promoting diversity and inclusion in their organization. Every group has a Teams channel associated with it where members can chat and post resources and events. The app's bot facilitates matching group members, setting up introductory meetings, and answering employee questions about diversity programs by querying your org's knowledge base. In the event an employee doesn't find a group that meets their interests, they can request to have a new one created"
  },
  "accentColor": "#737373",
  "bots": [
    {
      "botId": "<<botId>>",
      "scopes": [
        "personal",
        "team"
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "configurableTabs": [
    {
      "configurationUrl": "https://<<appDomain>>/ergChannelConfigTab",
      "canUpdateConfiguration": true,
      "scopes": [
        "team"
      ]
    }
  ],
  "staticTabs": [
    {
      "entityId": "Discover",
      "name": "Discover groups",
      "contentUrl": "https://<<appDomain>>/discover",
      "websiteUrl": "https://<<appDomain>>/discover",
      "scopes": [
        "personal"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<<appDomain>>"
  ],
  "webApplicationInfo": {
    "id": "<<botId>>",
    "resource": "api://<<appDomain>>"
  }
}