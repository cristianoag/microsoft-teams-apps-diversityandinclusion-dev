{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0.0",
  "id": "0c7397fc-9062-4eed-a16b-d8da7c330b8a",
  "packageName": "com.microsoft.teams.diconnect.comms",
  "developer": {
    "name": "<<companyName>>",
    "websiteUrl": "<<websiteUrl>>",
    "privacyUrl": "<<privacyUrl>>",
    "termsOfUseUrl": "<<termsOfUseUrl>>"
  },
  "localizationInfo": {
    "defaultLanguageTag": "en"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Diversity and Inclusion(Comms)",
    "full": "Diversity and Inclusion (Comms)"
  },
  "description": {
    "short": "A tool for communication and settings management for Diversity and Inclusion app",
    "full": "Your organization's diversity and inclusion leadership team can create and send communications to multiple teams or a large number of individual employees over Teams chat. Manage settings for the Diversity and Inclusion app and oversee existing and new employee groups as needed"
  },
  "accentColor": "#737373",
  "configurableTabs": [
    {
      "configurationUrl": "https://<<appDomain>>/configtab",
      "canUpdateConfiguration": true,
      "scopes": [
        "team"
      ]
    }
  ],
  "bots": [
    {
      "botId": "<<botId>>",
      "scopes": [
        "team"
      ],
      "supportsFiles": true,
      "isNotificationOnly": false
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<<appDomain>>"
  ],
  "webApplicationInfo": {
    "id": "<<botId>>",
    "resource": "api://<<appDomain>>"
  }
}